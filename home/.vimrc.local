scriptencoding utf-8
set encoding=utf-8
syntax on
set background=dark
colorscheme onedark

" markdown folding
let g:vim_markdown_folding_disabled = 1
set nobackup
set noswapfile
set nowritebackup
set modelines=0
set nocompatible
set autoread
set tabstop=2
set shiftwidth=2
set softtabstop=2
set expandtab
